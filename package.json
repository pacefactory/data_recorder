{
  "name": "data_recorder",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "pbuf-build": "pbjs -t static-module -w commonjs -o proto/DataStore.js proto/DataStore.proto && pbts -o proto/DataStore.d.ts proto/DataStore.js && mkdir -p dist/proto && cp proto/*.js dist/proto/",
    "build": "npm run pbuf-build && tsc",
    "start": "node dist/src/index.js",
    "test": "jest",
    "lint": "eslint .",
    "format": "prettier --write .",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.13",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "husky": "^9.1.7",
    "jest": "^30.0.4",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "protobufjs-cli": "^1.1.3",
    "ts-jest": "^29.4.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.36.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": ["eslint --fix", "prettier --write"],
    "*.{js,jsx}": ["prettier --write"],
    "*.{md,json}": ["prettier --write"]
  },
  "dependencies": {
    "mqtt": "^5.14.1",
    "protobufjs": "^7.5.4"
  }
